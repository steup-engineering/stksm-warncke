        <halJsonPin name="dcms" type="u32" dir="in"/>
      </halJsonObject>
    </halJsonArray>
    <halJsonArray name="rolls" size="31">
      <halJsonPin name="active" type="bit" dir="in"/>
    </halJsonArray>
    <halJsonArray name="cleaners" size="3">
      <halJsonPin name="active" type="bit" dir="in"/>
      <halJsonObject name="whm">
        <halJsonPin name="secs" type="u32" dir="in"/>
        <halJsonPin name="dcms" type="u32" dir="in"/>
