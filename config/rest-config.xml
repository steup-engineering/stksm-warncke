    <halJsonArray name="rolls" size="31">
      <halJsonPin name="manu" type="bit" dir="out"/>
      <halJsonPin name="ena" type="bit" dir="out"/>
    </halJsonArray>
    <halJsonArray name="cleaners" size="3">
      <halJsonPin name="manu" type="bit" dir="out"/>
      <halJsonPin name="ena" type="bit" dir="out"/>
